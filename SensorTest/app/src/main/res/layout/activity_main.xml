<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.a1a1a1.sensortest.MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="Gravity Values"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/g_x1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:text="NO RETURN" />
    <TextView
        android:id="@+id/g_x2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_below="@+id/textView2"
        android:layout_toRightOf="@+id/textView5"
        android:layout_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/g_x3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_alignTop="@+id/g_x2"
        android:layout_toRightOf="@+id/l_x3"
        android:layout_toEndOf="@+id/l_x3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Accel"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_below="@+id/g_x1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="93dp" />

    <TextView
        android:id="@+id/a_x1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:id="@+id/a_x2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_alignTop="@+id/a_x1"
        android:layout_toRightOf="@+id/btn_start"
        android:layout_toEndOf="@+id/btn_start" />

    <TextView
        android:id="@+id/a_x3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_alignTop="@+id/a_x2"
        android:layout_toRightOf="@+id/g_x2"
        android:layout_toEndOf="@+id/g_x2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Linear Accel"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/l_x1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:id="@+id/l_x2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_alignTop="@+id/l_x1"
        android:layout_alignLeft="@+id/a_x2"
        android:layout_alignStart="@+id/a_x2" />

    <TextView
        android:id="@+id/l_x3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_alignTop="@+id/l_x2"
        android:layout_toRightOf="@+id/g_x2"
        android:layout_toEndOf="@+id/g_x2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gyro"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_marginTop="57dp"
        android:layout_below="@+id/l_x1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/gy_x1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/gy_x2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_alignTop="@+id/gy_x1"
        android:layout_alignLeft="@+id/l_x2"
        android:layout_alignStart="@+id/l_x2" />

    <TextView
        android:id="@+id/gy_x3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO RETURN"
        android:layout_alignTop="@+id/gy_x2"
        android:layout_alignLeft="@+id/l_x3"
        android:layout_alignStart="@+id/l_x3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="시작"
        android:id="@+id/btn_start"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="종료"
        android:id="@+id/button2"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/g_x3"
        android:layout_alignEnd="@+id/g_x3" />


</RelativeLayout>
